# meta developer: @Gos_HolY
from asyncio import sleep

from .. import loader


@loader.tds
class penisa(loader.Module):
    strings = {"name": "Песня Большие Пениса Текстом"}

    @loader.owner
    async def penisacmd(self, message):
        for _ in range(1):
            for penisa in [
                "Большие Пениса👉👌",
                "Стоят Вокруг Меня🌝",
                "И Спермой Нихуя💦",
                "Уж Всё Внутри Меня🌚",
                "Прям Попкой На ПЭНСИЛ🍑",
                "Я Знаю Ты Любил🌚",
                "Садиться На Мой Член🌚",
                "И Сперма По ЩЕКЕЕЕЕЕЕЕ💦🌝",
            ]:
                await message.edit(penisa)
                await sleep(5.1)
